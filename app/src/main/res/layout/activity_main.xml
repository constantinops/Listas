<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="147dp"
        android:layout_height="150dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="127dp"
        android:layout_marginTop="129dp"
        android:layout_marginEnd="137dp"
        app:srcCompat="@android:drawable/sym_def_app_icon" />

    <Button
        android:id="@+id/buttonListaSimple"
        android:layout_width="134dp"
        android:layout_height="wrap_content"
        android:layout_above="@+id/buttonsql"
        android:layout_below="@+id/imageView"
        android:layout_alignParentStart="true"
        android:layout_marginStart="50dp"
        android:layout_marginTop="64dp"
        android:layout_marginBottom="48dp"
        android:text="Lista Simple"

        />

    <Button
        android:id="@+id/buttonListaPerso"
        android:layout_width="165dp"
        android:layout_height="wrap_content"
        android:layout_above="@+id/buttonsql"
        android:layout_below="@+id/imageView"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="25dp"
        android:layout_marginTop="62dp"
        android:layout_marginEnd="61dp"
        android:layout_marginBottom="43dp"
        android:layout_toEndOf="@+id/buttonListaSimple"
        android:text="Lista Personalizada "

        />

    <Button
        android:id="@+id/buttonsql"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="146dp"
        android:layout_marginEnd="177dp"
        android:layout_marginBottom="117dp"
        android:text="bdSQL"
        android:onClick="onClick"
        />
</RelativeLayout>